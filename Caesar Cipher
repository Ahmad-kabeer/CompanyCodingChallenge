#include <bits/stdc++.h>

 
using namespace std;
// Solved By Pappu Kumar (Coder Guy)

string helper(string s)
{
    string result = ""; // answer store string

    for(int i=0;i<s.length();i++) // loop through
 the input string

    {
        char ch = char((s[i] + 3 - 97)%26 + 97);

        result+=ch;

    }

    return result;

}
int main() {

    string s;

    cin>>s;

    cout<<helper(s);

 

    return 0;


}
